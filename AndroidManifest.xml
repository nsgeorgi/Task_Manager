<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.task_manager"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"></uses-permission>
    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Splash"
             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
         <activity
            android:name="com.example.task_manager.General_Information"
             >
            <intent-filter>
                <action android:name="com.example.task_manager.GENERAL_INFORMATION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
         
         <activity
            android:name=".Menu"
             >
            <intent-filter>
                <action android:name="com.example.task_manager.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
          
         <activity
            android:name=".Service"
             >
            <intent-filter>
                <action android:name="com.example.task_manager.SERVICE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         
         <activity
            android:name=".Kill_proc"
             >
            <intent-filter>
                <action android:name="com.example.task_manager.KILL_PROC" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
          <activity
            android:name=".Ram"
             >
            <intent-filter>
                <action android:name="com.example.task_manager.RAM" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         
    </application>

</manifest>